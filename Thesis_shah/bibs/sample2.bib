pages = {3702--3704},
pmid = {15284097},
title = {{CLANS: a Java application for visualizing protein families based on pairwise similarity}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15284097 https://academic.oup.com/bioinformatics/article-lookup/doi/10.1093/bioinformatics/bth444},
